{
    "version": 2,
    "builds": [
        { "src": "api/gas.js", "use": "@vercel/node" },
        { "src": "api/gasCocina.js", "use": "@vercel/node" },
        { "src": "api/login.js", "use": "@vercel/node" }
    ],
    "routes": [
        { "src": "/api/gas", "dest": "api/gas.js" },
        { "src": "/api/gasCocina", "dest": "api/gasCocina.js" },
        { "src": "/api/login", "dest": "api/login.js" }
    ]
}